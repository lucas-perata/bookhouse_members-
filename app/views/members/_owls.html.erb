<div class="column is-half is-centered">
<div>
                <p class="title is-4 is-danger">Your owls:</p>
                </div>
                <% @owls.each do |owl| %>
                 <% if owl.member.username == current_member.username %>
                  
                 
                       <div class="box">
                        <div class="media">
                            <div class="media-left">
                                <div class="image is-64x64">
                                    <img src="https://bulma.io/images/placeholders/64x64.png" alt="placeholder">
                                </div>
                                <div class="media-content">
                                    <div class="content">
                                        <strong> <%= owl.member.name %> </strong> <br>
                    
                    <small><%= owl.member.username %></small> <br>
                   
                    <p> <%= owl.owl %></p>
                                    </div>
                                </div>
                            </div>
                        </div>
                       </div>
                      
                            
                        <% end %>
                        <% end %>
             </div>